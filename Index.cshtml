    <script>
        $(document).ready(function () {
            $('.navbar-nav a').each(function () {
                if ($(this).text() == $('#page-indicator').val()) {
                    $(this).addClass('active');
                }
            });

            $(".onHoverChangeColorBlue").hover(function () {
                $(this).css("color", "#337ab7")
            }, function () {
                $(this).css("color", "#000000")
            });
        });
    </script>
